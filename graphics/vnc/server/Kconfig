menuconfig VNCSERVER
	bool "VNC server"
	default n
	depends on NET_TCP && !NX_LCDDRIVER && EXPERIMENTAL
	---help---
		Enable support for a VNC Remote Frame Buffer (RFB) server.

if VNCSERVER

endif # VNCSERVER
