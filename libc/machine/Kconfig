	bool
	default n
	depends on LIBM && ARCH_CORTEXM33

config LIBM_ARCH_FLOORF
	bool
	default n
	depends on LIBM && ARCH_CORTEXM33

config LIBM_ARCH_NEARBYINTF
	bool
	default n
	depends on LIBM && ARCH_CORTEXM33

config LIBM_ARCH_RINTF
	bool
	default n
	depends on LIBM && ARCH_CORTEXM33

