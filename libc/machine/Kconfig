	bool
	default n

config LIBC_ARCH_STRNCPY
	bool
	default n

config LIBC_ARCH_STRLEN
	bool
	default n

config LIBC_ARCH_STRNLEN
	bool
	default n

config LIBC_ARCH_BZERO
	bool
	default n

config LIBM_ARCH_CEIL
	bool
	default n
	depends on LIBM && ARCH_CORTEXM33

