	bool
	default n
	depends on LIBM && ARCH_CORTEXM33

config LIBM_ARCH_ROUND
	bool
	default n
	depends on LIBM && ARCH_CORTEXM33

config LIBM_ARCH_TRUNC
	bool
	default n
	depends on LIBM && ARCH_CORTEXM33

