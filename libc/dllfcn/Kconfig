config LIBC_DLLFCN
	bool "Shared library support"
	default n
	select LIBC_ARCH_ELF
	depends on EXPERIMENTAL
	---help---
		Enabled support for user-space shared libraries.

		A work in progress, hence, marked EXPERIMENTAL
